<html>
<head>
	<title>mschedule.com :: its like google, for wolverineaccess</title>
	<link rel="stylesheet" href="{{$cfg.ms_rootpath.client}}/stylesheets/main.css">
	{{if $cfg.stopSearchEngineIndexing}}
		<meta name="robots" content="noindex,nofollow,noarchive">
	{{/if}}
</head>
<body>

{{* INITIALIZE ALL POPUPS *}}
{{popup_init src=$cfg.smarty.overlibPath}}

<center>

<div style="height: 10px;"><!-- spacer --></div>

<div id="globalcontainer">

	<div id="topbar">
		<a href="{{$cfg.ms_rootpath.client}}"><img src="{{$cfg.ms_rootpath.client}}/images/topbar.jpg"></a>
	</div>

	<!--begin login bar-->
	<div style="width:800px"> <!-- IE 5.0 fix -->

		<div id="loginbar" align="right">
			{{$loginbar}}
		</div>
	
	</div>
	<!--end login bar-->
	
		<div id="secondarycontainer">

			{{if $cfg.showErrors=="yes" && $MSERROR->num gt 0}}
				<div id="errorbox" align="left">

					<table width="100%">
					{{foreach from=$MSERROR->messages item=message}}
						<tr>
							<td width="200"><b>{{$message.origin}}</b></td>
							<td>{{$message.msg}}</td>
						</tr>
					{{/foreach}}
					</table>
					
				</div>
				
				<br>
			{{/if}}

			{{if $cfg.showDebug=="yes" && $MSDEBUG->num gt 0}}
				<div id="debugbox" align="left">

					<table width="100%">
					{{foreach from=$MSDEBUG->messages item=message}}
						<tr>
							<td width="200"><b>{{$message.origin}}</b></td>
							<td>{{$message.msg}}</td>
						</tr>
					{{/foreach}}
					</table>
					
				</div>
				
				<br>
			{{/if}}
			
			<table style="width: 100%;">
				<tr>
					<!--begin left sidebar-->
					<td style="width: 150px" valign="top">
													
						{{foreach from=$menuboxes item=menu}}
							<!--begin menu box-->
							{{include file="elements/menubox.shtml" menuvars=$menu}}
							<!--end menu box-->
							<br>
						{{/foreach}}
					</td>
					<!--end left sidebar-->

					<td style="width:20px"><!--spacer--></td>

					<!--begin content area-->
					<td valign="top">
					
						<center>
							<h2>{{$pagetitle}}</h2>
							<br>
						</center>
						
						<!--begin general text box-->
						<div class="outerbox">
							<div class="textbox">
							
								<div class="normaltext">
								{{$content}}
								</div>
								
							</div>
						</div>
						<!--end general text box-->
					</td>
					<!--end content area-->
				</tr>
			</table>
			
		</div>
		</div>
	
	
</div>
</center>
</body>
</html>